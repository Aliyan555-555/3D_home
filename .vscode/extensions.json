    "**/.git/subtree-cache/**": true,
    "**/node_modules/**": true,
    "env-*": true
  },
  "files.exclude": {
    "**/.git": true,
    "**/.DS_Store": true,
    "**/node_modules": true,
    "env*": true
  }  
}